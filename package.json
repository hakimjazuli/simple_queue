{
	"name": "@html_first/simple_queue",
	"version": "1.3.0",
	"description": "base queue handler for @html_first packages",
	"main": "index.mjs",
	"types": "./types/index.d.mts",
	"module": "index.mjs",
	"scripts": {
		"dev": "concurrently \"bun ./dev/index.mjs\" \"bun gen:type\"",
		"gen:type": "tsc ./index.mjs --allowJs --declaration --emitDeclarationOnly --outDir ./types --module ESNext --target ES2020 --moduleResolution node --esModuleInterop --skipLibCheck --watch"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hakimjazuli/simple_queue.git"
	},
	"keywords": [
		"simple queue handler for frontend or backend js"
	],
	"author": "hakim_jazuli",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/hakimjazuli/simple_queue/issues"
	},
	"homepage": "https://github.com/hakimjazuli/simple_queue#readme",
	"devDependencies": {
		"@html_first/js_lib_template": "link:@html_first/js_lib_template"
	},
	"dependencies": {}
}
